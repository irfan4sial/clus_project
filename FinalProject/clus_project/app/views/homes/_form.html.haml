.clearfix
.modal-dialog.home_dialog_modal
  .modal-content
    .modal-header
      .modal-title
        %h5 Modal title
    .modal-body.home_register
      = form_for(@home, :remote => true) do |f|
        %div.left_panel
          .control-group
            .control-label
              = f.label :name
            .controls
              = f.text_field :name
        %div.right_panel
          .control-group
            .control-label
              = f.label :image
            .controls
              = f.file_field :image
        .field
          = f.submit :id => "homes_btn_id"
    .modal-footer
      %a.btn.primary{:href => "#"} Done
