.container
  / ----
  #content
    %ul#tabs.nav.nav-tabs{"data-tabs" => "tabs"}
      %li.active
        %a{"data-toggle" => "tab", :href => "#red"} Red
      %li
        %a{"data-toggle" => "tab", :href => "#orange"} Orange
    #my-tab-content.tab-content
      #red.tab-pane.active
        -#= render 'registrations/new'
      #orange.tab-pane
        = render 'homes/form'
  :javascript
    jQuery(document).ready(function ($) {
        $('#tabs').tab();
    });
/ container
